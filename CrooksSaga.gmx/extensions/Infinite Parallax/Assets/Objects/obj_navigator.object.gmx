            <string>/// move around

// slow down 
velX *= 0.85; 
velY *= 0.85; 

if (abs(velX) &lt; 0.125) then velX = 0; 
if (abs(velY) &lt; 0.125) then velY = 0; 

// now take input
if keyboard_check(vk_left) then velX -= rate/2; 
if keyboard_check(vk_right) then velX += rate/2; 
if keyboard_check(vk_up) then velY -= rate/2; 
if keyboard_check(vk_down) then velY += rate/2;

// constrain total speed to rate
if (abs(velX) &gt;= rate || abs(velY) &gt;= rate) {
    var dist = point_distance(0,0,velX,velY); 

    velX = velX/dist * rate; // cos(dir) * speed
    velY = velY/dist * rate; // sin(dir) * speed
}

// update actual position 
x += velX; 
y += velY; 

//show_debug_message(string(point_distance(xprevious,yprevious,x,y))); // output magnitude



</string>
